{% block title %}Upload test{% endblock %}

{% block description %}This page will render a simple form to upload a file.{% endblock %}

{% block content %}
  <div>
    <div class="row">
      <div class="col-lg-8">
      {{ form_start(form) }}
        <div class="form-group">
          {{ form_label(form.uploadFile) }}
          {{ form_widget(form.uploadFile) }}
          {{ form_errors(form.uploadFile) }}
        </div>
        <div class="form-group">
          {{ form_widget(form.send) }}
        </div>
        {{ form_rest(form) }}
      {{ form_end(form) }}
      </div>
    </div>
  </div>
{% endblock %}